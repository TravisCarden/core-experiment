{
    "name": "n/a",
    "description": "A project template for doing Drupal core and contrib work.",
    "license": "proprietary",
    "require": {
        "acquia/composer-installer-plugin": "dev-composer-plugin",
        "drupal/core": "8.8.2",
        "drupal/checklistapi": "1.x-dev",
        "composer/installers": "^1.0@dev"
    },
    "config": {
        "preferred-install": {
            "drupal/drupal": "source"
        }
    },
    "repositories": {
        "composer-plugin": {
            "type": "path",
            "url": "composer-installer-plugin"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev"
}
