{
    "name": "drupal/contributor-project",
    "description": "A project template for doing Drupal core and contrib work.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "drupal/installer-plugin": "*",
        "composer/installers": "^1.0@dev",
        "cweagans/composer-patches": "^1.6.7",
        "drupal/drupal": "8.8.2"
    },
    "config": {
        "preferred-install": {
            "drupal/drupal": "source"
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-disable": ["drupal"]
    },
    "repositories": {
        "installer-plugin": {
            "type": "path",
            "url": "../drupal-contrib-project/installer-plugin"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "drupal-core": {
            "type": "path",
            "url": "web/core"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev"
}
